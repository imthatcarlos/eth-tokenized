{
  "name": "eth-tokenized",
  "version": "0.0.1",
  "description": "collection of smart contracts to tokenize assets",
  "main": "truffle.js",
  "scripts": {
    "0x:trace": "MODE=trace truffle test",
    "0x:coverage": "npm run clean && MODE=coverage truffle test; istanbul report html && open coverage/index.html",
